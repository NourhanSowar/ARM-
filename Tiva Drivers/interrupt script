//notes

void isr_portF(void)    //Interrupt.c  >>  as isr function   da li hktbo fl startup
{
   registered_interrupt_portF; // pointer to function  in .h   void (*interrupt_to_register)(void)  hn5leh y4awr 3l fun li user katbha as isr
}

//****************


void Int_Reg(int interrupt_ID, void (*interrupt_to_register)(void))  //Interrupt.h   // bdelo  isr li 3ayzo y48lo lma y7sl interrupt
{
    if (interrupt_ID == 30)
        registered_interrupt_portF = interrupt_to_register;
}
